
all:	DCC DCPP DC1 DAS DLINK DOBJ DUTIL

DCC:
	(cd dcc; dxmake)

DCPP:
	(cd dcpp; dxmake)

DC1:
	(cd dc1; dxmake)

DAS:
	(cd das; dxmake)

DLINK:
	(cd dlink; dxmake)

DOBJ:
	(cd dobj; dxmake)

DUTIL:
	(cd dutil; dxmake)

install:
	(cd dcc; dxmake install)
	(cd dcpp; dxmake install)
	(cd dc1; dxmake install)
	(cd das; dxmake install)
	(cd dlink; dxmake install)
	(cd dobj; dxmake install)
	(cd dutil; dxmake install)

clean:
	(cd dcc; dxmake clean)
	(cd dcpp; dxmake clean)
	(cd dc1; dxmake clean)
	(cd das; dxmake clean)
	(cd dlink; dxmake clean)
	(cd dobj; dxmake clean)
	(cd dutil; dxmake clean)

