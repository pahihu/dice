		;
		;   D0/D1 -> D0 signed divide
		;   32/32 -> 32 signed divide

		section text,code

		xdef	__divs
		xref	__divu

__divs:
		tst.l	D1
		bmi	ds10
		tst.l	D0	     ; numerator
		bmi	ds20
		bra	__divu
ds10		neg.l	D1
		tst.l	D0
		bmi	ds30
ds10n		bsr	__divu
		neg.l	D0
		rts
ds20		neg.l	D0
		bsr	__divu
		neg.l	D0
		rts
ds30		neg.l	D0
		bra	__divu

		END

