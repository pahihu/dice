		section text,code

		xref	_MathBase
		xref	_LVOSPCmp
		xdef	__ffpcmp_a

__ffpcmp_a	move.l	A6,-(sp)
		move.l	_MathBase(A4),A6
		exg	D0,D1		; for TST, result reversed!
		jsr	_LVOSPCmp(A6)
		move.l	(sp)+,A6
		tst.l	D0
		rts

		END

