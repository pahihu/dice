		    xdef    _hyper_memset
		    xdef    _memset
		    xdef    @memset
		    xref    _setmem
		    xref    @setmem

		    section text,code

@memset:	    exg     D0,D1
		    jmp     @setmem(pc)

_hyper_memset:
_memset:
		    lea     8(sp),A0
		    movem.l (A0),D0/D1
		    exg     D0,D1
		    movem.l D0/D1,(A0)
		    jmp     _setmem(pc)

		    END

